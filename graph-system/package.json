{
  "name": "enhanced-meeting-intelligence-graph",
  "version": "1.0.0",
  "description": "Graph-based Enhanced Meeting Intelligence System using Neo4j",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/ tests/",
    "lint:fix": "eslint src/ tests/ --fix",
    "db:setup": "node scripts/setup-database.js",
    "db:seed": "node scripts/seed-data.js",
    "db:migrate": "node scripts/migrate.js",
    "build": "echo 'Build completed'",
    "docker:build": "docker build -t meeting-intelligence-graph .",
    "docker:run": "docker-compose up -d"
  },
  "keywords": [
    "neo4j",
    "graph-database",
    "meeting-intelligence",
    "enterprise",
    "analytics",
    "temporal-queries"
  ],
  "author": "Enhanced Meeting Intelligence Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "neo4j-driver": "^5.15.0",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "joi": "^17.11.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "express-rate-limit": "^7.1.5",
    "winston": "^3.11.0",
    "dotenv": "^16.3.1",
    "uuid": "^9.0.1",
    "moment": "^2.29.4",
    "lodash": "^4.17.21",
    "express-validator": "^7.0.1",
    "multer": "^1.4.5-lts.1",
    "csv-parser": "^3.0.0",
    "node-cron": "^3.0.3"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "nodemon": "^3.0.2",
    "eslint": "^8.55.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.0",
    "prettier": "^3.1.1",
    "@types/jest": "^29.5.8"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/tests/**/*.test.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/app.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
